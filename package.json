{
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write .",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NEST_DEBUG=true NODE_ENV=development nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint './**/*.ts'",
    "compile": "tsc",
    "test": "jest",
    "test:colors": "jest --colors",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@contentful/rich-text-types": "15.1.0",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.0",
    "@nestjs/core": "^8.2.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.3",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.0",
    "@nestjs/typeorm": "^8.0.2",
    "@slack/webhook": "6.0.0",
    "aws-sdk": "2.1027.0",
    "cache-manager": "3.4.4",
    "cheerio": "1.0.0-rc.10",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "contentful": "8.4.2",
    "contentful-management": "7.7.0",
    "contentful-typescript-codegen": "3.2.2",
    "cron-parser": "4.1.0",
    "dotenv": "10.0.0",
    "ejs": "3.1.6",
    "form-data": "4.0.0",
    "fs": "0.0.1-security",
    "helmet": "4.6.0",
    "html-node": "0.1.1",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.33",
    "mysql": "2.18.1",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "8.1.6",
    "node-mailjet": "3.3.4",
    "passport": "0.5.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "puppeteer": "10.4.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "^7.4.0",
    "typeorm": "0.2.25",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.2.0",
    "@types/cache-manager": "3.4.2",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.32",
    "@types/node-mailjet": "3.3.7",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "env-cmd": "10.1.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "setupFiles": [
      "<rootDir>/utils/jest/setEnvVars.ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14.15.1"
  }
}
